<?xml version="1.0" encoding="utf-8"?>
<resources>

    <item name="listChildTag" type="id" />

    <declare-styleable name="PasswordInputView">
        <attr name="borderColor" format="color" />
        <attr name="contentColor" format="color" />
        <attr name="borderRadius" format="dimension" />
        <attr name="passwordLength" format="integer" />
        <attr name="passwordColor" format="color" />
        <attr name="passwordRadius" format="dimension" />
        <attr name="cellWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TabLayout">
        <attr name="indicator" format="color|reference" />
        <attr name="showIndicator" format="boolean" />
        <attr name="indicateHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="TabItem">
        <attr name="itemTextSize" format="dimension"/>
        <attr name="itemChecked" format="boolean"/>
        <attr name="itemTextColors" format="color"/>
        <attr name="itemText" format="string"/>
    </declare-styleable>
    <declare-styleable name="LockView">
        <attr name="src" format="reference" />
        <attr name="verticalSpacing" format="dimension" />
        <attr name="horizontalSpacing" format="dimension" />
        <attr name="password" format="string" />
        <attr name="lineColor" format="reference" />
        <attr name="failColor" format="color" />
        <attr name="successColor" format="color" />
        <attr name="layoutSquare" format="enum">
            <enum name="width" value="1" />
            <enum name="height" value="2" />
            <enum name="shorter" value="3" />
            <enum name="longer" value="4" />
        </attr>
    </declare-styleable>


    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="SupperLayout">
        <attr name="space" format="dimension"/>
        <attr name="layout_width_units" format="integer"/>
        <attr name="layout_height_units" format="integer|enum">
            <enum name="auto" value="-1"/>
        </attr>

    </declare-styleable>
    <declare-styleable name="SupperLayout_Layout">
        <attr name="x" format="integer"/>
        <attr name="widthUnits" format="integer"/>
        <attr name="y" format="integer"/>
        <attr name="heightUnits" format="integer"/>
    </declare-styleable>

    <declare-styleable name="RoundCornerImageView">
        <attr name="x_radius" format="dimension"/>
        <attr name="y_radius" format="dimension"/>
    </declare-styleable>

</resources>